curl -s -X POST 'https://api.etherscan.io/v2/api?chainid=560048' \
-H 'Content-Type: application/x-www-form-urlencoded' \
--data-urlencode 'module=contract' \
--data-urlencode 'action=verifysourcecode' \
--data-urlencode 'apikey=W3HFCB87IWVV6PK7ITW8287FAU367ESMVA' \
--data-urlencode 'contractaddress=0x96C326866dB1B879B2a25bE4104Fd1d2a7FFb108' \
--data-urlencode 'codeformat=solidity-single-file' \
--data-urlencode 'contractname=TestVerification' \
--data-urlencode 'compilerversion=v0.7.6+commit.7338295f' \
--data-urlencode 'optimizationUsed=1' \
--data-urlencode 'runs=200' \
--data-urlencode '' \
--data-urlencode 'sourceCode=// SPDX-License-Identifier: MIT
pragma solidity =0.7.6;

contract TestVerification {
string public constant name = "Test Contract";
uint256 public immutable deployTime;
string private _name;
string private _symbol;
uint8 private _decimals;

constructor(
string memory name_,
string memory symbol_,
uint8 decimals_
) {
deployTime = 20251224005;
_name = name_;
_symbol = symbol_;
_decimals = decimals_;
}

function getMessage() external pure returns (string memory) {
return "Hello Conflux!";
}
}'
